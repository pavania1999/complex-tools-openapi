{
    "name": "enum-nested-api",
    "version": "1.0.0",
    "description": "Local MCP Server for Enum Nested Schemas API with direct implementation",
    "private": true,
    "type": "module",
    "bin": {
        "enum-nested-api": "./build/index.js"
    },
    "files": [
        "build"
    ],
    "scripts": {
        "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
        "prepare": "npm run build",
        "watch": "tsc --watch",
        "inspector": "npx @modelcontextprotocol/inspector build/index.js"
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "enum-validation",
        "nested-schemas"
    ],
    "author": "IBM Bob",
    "license": "MIT",
    "dependencies": {
        "@modelcontextprotocol/sdk": "0.6.0"
    },
    "devDependencies": {
        "@types/node": "^20.11.24",
        "typescript": "^5.3.3"
    }
}